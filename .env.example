DISCORD_TOKEN=your-bot-token-here
CLIENT_ID=your-application-client-id

# Set to development to deploy commands to a single guild (faster)
# Set to production (or leave unset) to deploy globally
NODE_ENV=production

# Only required when NODE_ENV=development
GUILD_ID=your-test-guild-id

# Comma or space separated list of owner IDs
BOT_OWNER_IDS=123456789012345678,987654321098765432

# Optional: only needed for /removebg
REMOVE_BG_API_KEY=your-removebg-api-key
